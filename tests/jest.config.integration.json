{
   "rootDir": "../",
   "verbose": false,
   "testURL": "http://wpshopify.loc",
   "modulePathIgnorePatterns": ["<rootDir>/node_modules", "<rootDir>/vendor", "<rootDir>/.git"],
   "roots": ["<rootDir>/tests", "<rootDir>/src"],
   "testEnvironment": "node",
   "notify": true,
   "setupFiles": ["<rootDir>/tests/jest-test-env.js"],
   "setupFilesAfterEnv": ["<rootDir>/tests/jest-test-setup.js"],
   "testRegex": "integration.tests.js"
}
